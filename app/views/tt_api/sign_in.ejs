<div class="jumbotron">
  <h1>Sign in</h1>
  <% if (error){ %>
    <div class="error">
      <%= error %>
    </div>
  <% } %>
  <form action="/tt_login" method="post">
    <div>
        <p></p>
        <label>Email:</label>
        <input type="text" name="email"/>
    </div>
    <div>
        <label>Password:</label>
        <input type="password" name="password"/>
    </div>
    <div>
        <input type="submit" class="btn btn-primary btn-sm" value="Log In"/>
    </div>
  </form>

</div>